{
  "name": "na-map-data",
  "version": "0.0.0",
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "typecheck": "vue-tsc --noEmit",
    "update": "yarn set version stable && yarn upgrade-interactive"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/node": "^20.4.2",
    "@types/prettier-package-json": "^2.3.0",
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.0",
    "prettier-package-json": "^2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "eslint-plugin-prettier": "^5.0.0-alpha.2"
  },
  "engines": {
    "node": ">= 18"
  },
  "browserslist": [
    "defaults and supports es6-module and last 3 versions, maintained node versions"
  ],
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "tabWidth": 4
  },
  "prettier-package-json": {
    "tabWidth": 4
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case",
          "upper-case"
        ]
      ],
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ]
    }
  }
}
